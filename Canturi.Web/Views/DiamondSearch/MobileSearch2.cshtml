        function FnSliderCarat(minVal, maxVal, isDisabled) {
            var caratMin = $("#hdnMinimumCaratSize").val();
            var caratMax = $("#hdnMaximumCaratSize").val();
            $("#slider-carat").slider({
                disabled: isDisabled,
                range: true,
                min: parseFloat(caratMin),
                max: parseFloat(caratMax),
                step: .01,
                values: [minVal, maxVal],
                stop: function (event, ui) {
                    document.getElementById("hdnTmpCaratMin").value = ui.values[0];
                    document.getElementById("hdnTmpCaratMax").value = ui.values[1];

                },

                slide: function (event, ui) {
                    $("#carat-min").val(ui.values[0]);
                    $("#carat-max").val(ui.values[1]);
                }
            });
            $("#carat-min").val($("#slider-carat").slider("values", 0));
            $("#carat-max").val($("#slider-carat").slider("values", 1));
            //$("#carat-min").attr("readonly", "readonly");
            //$("#carat-max").attr("readonly", "readonly");
            $('.Range a:last-child').addClass('handle-2');
            if (isDisabled) {
                $("#carat-min").attr("disabled", "disabled");
                $("#carat-max").attr("disabled", "disabled");
            }
            else {
                $("#carat-min").removeAttr("disabled");
                $("#carat-max").removeAttr("disabled");
            }
