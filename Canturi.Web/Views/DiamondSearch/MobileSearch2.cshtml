    @*<script type="text/javascript" src="@Url.Content("~/Content/FrontEnd/js/jquery.countdownTimer.js")"></script>*@
    <script type="text/javascript">
        var largestDiamondPercentage = '@ViewBag.LargestDiamondPercentage';
        $(window).load(function () {
           
            if ($("#hdnSearchClick").val() == 1) {

                var diamondShape = $("#hdnDiamondShape").val();
                var isClarity1 = false;
                if (diamondShape != 'Round') {
                    FnSliderCut(1, 4, true);
                }
                else {
                    FnSliderCut($("#hdnCutMin").val(), $("#hdnCutMax").val(), false);
                }


                if (diamondShape != 'Emerald') {

                    isClarity1 = false;
                }
                else {
                    isClarity1 = true;
                }

                if (!isClarity1) {
                    if (diamondShape != 'Asscher') {
                        isClarity1 = false;
                    }
                    else {
                        isClarity1 = true;
                    }
                }
                if ($("#hdnTmpIsFindLargestDiamond").val() == "0") {
                    FnSliderCarat($("#hdnCaratMin").val(), $("#hdnCaratMax").val(), false);
                    FnSliderPrice($("#hdnPriceMinVal").val(), $("#hdnPriceMaxVal").val(), false);
                }
                else {

                    var hdnMinimumCaratSize = $("#hdnMinimumCaratSize").val();
                    var hdnMaximumCaratSize = $("#hdnMaximumCaratSize").val();
                    FnSliderCarat(hdnMinimumCaratSize, hdnMaximumCaratSize, false);
                    FnSliderPrice(0, 132, true);
                }



                FnSliderColour($("#hdnColorMin").val(), $("#hdnColorMax").val());

                FnSliderPolish($("#hdnPolishMin").val(), $("#hdnPolishMax").val());
                FnSliderSymmetry($("#hdnSymmetryMin").val(), $("#hdnSymmetryMax").val());
                FnSliderFluorescence($("#hdnFlourscenceMin").val(), $("#hdnFlourscenceMax").val());




                if ($("#hdnClarityClick").val() == 1) {

                    $("#chkEyeClean").removeAttr("disabled").removeAttr("checked", "");
                    $(".chkMobEyeClean").removeAttr("disabled").removeAttr("checked", "");

                    if ($("#hdnIsEyeCleanClick").val() == "1") {
                        //$("#chkEyeClean").attr("checked", "checked");
                        $(".chkMobEyeClean").attr("checked", "checked");
                        $("#hdnIsEyeClean").val('true');
                        $("#hdnTmpIsEyeClean").val('true');
                    }



                    $(".chkClarity1").attr("checked", "checked");
                    $("#chkMobClarity1").attr("checked", "checked");
                    $(".chkClarity2").removeAttr("checked");

                    FnClaritySlider1($("#hdnClarityMin").val(), $("#hdnClarityMax").val(), false);

                }
                else {
                    //$(".chkClarity2").attr("checked", "checked");
                    //$("#chkMobClarity2").attr("checked", "checked");
                    $(".chkClarity2").click();
                    //$("#chkMobClarity2").click();
                    $(".chkClarity1").removeAttr("checked");
                    $("#chkMobClarity1").removeAttr("checked");

                    $("#slider-clarity").slider({ disabled: true });
                    $("#slider-clarity2").slider({ disabled: false });
                    $('.Range a:last-child').addClass('handle-2');
                    //$("#chkEyeClean").attr("disabled", "disabled").removeAttr("checked");
                    $(".chkMobEyeClean").attr("disabled", "disabled").removeAttr("checked");
                    //$("#chkEyeClean").attr("checked", "checked");
                    $(".chkMobEyeClean").attr("checked", "checked");

                    if (!isClarity1) {
                        FnClaritySlider1(1, 4, false);
                        FnClaritySlider2($("#hdnClarityMin2").val(), $("#hdnClarityMax2").val(), false);
                    }
                    else {

                        FnClaritySlider1(1, 4, true);
                        $(".chkClarity1").attr("disabled", "Disabled");
                        FnClaritySlider2($("#hdnClarityMin2").val(), $("#hdnClarityMax2").val(), false);
                    }

                }
                var hdnMinimumCaratSize = $("#hdnMinimumCaratSize").val();
                var hdnMaximumCaratSize = $("#hdnMaximumCaratSize").val();
                if ($("#chkPrice").is(':checked')) {
                    FnSliderPrice(0, 132, true);

                    FnSliderCarat('' + hdnMinimumCaratSize + '', '' + hdnMaximumCaratSize + '', true);
                    $("#txtPrice").removeAttr("disabled");
                    $("#hdnTmpIsFindLargestDiamond").val("1");
                }
                else {
                    $("#txtPrice").val("");
                    $("#txtPrice").attr("disabled", "disabled");
                    FnSliderCarat($("#hdnCaratMin").val(), $("#hdnCaratMax").val(), false);
                    FnSliderPrice($("#hdnPriceMinVal").val(), $("#hdnPriceMaxVal").val(), false);
                    $("#hdnTmpIsFindLargestDiamond").val("0");
                }

                SearchBackSelectDiamond("li" + $("#hdnDiamondShape").val());
                GetSearchData(document.getElementById("hdnCurrentPage").value, 1);
                BindSelection('', '', '');
            }

        });
        //$("#hdnSearchClick")

        $(document).ready(function () {
            $(".wrapper").bind("click touchstart mousedown touchend touchmove", function (e) {
                $(".listing-tab-box").hide();
