        function GetMinCaratSlider() {
            var caratMin = $("#hdnMinimumCaratSize").val();
            var caratMax = $("#hdnMaximumCaratSize").val();
            //alert(parseFloat(document.getElementById("carat-min").value));
            if (document.getElementById("carat-min").value == "") {
                document.getElementById("carat-min").value = document.getElementById("hdnTmpCaratMin").value;
                return;
            }
            if (parseFloat(document.getElementById("carat-min").value) < parseFloat(caratMin)) {
                document.getElementById("carat-min").value = caratMin;
            }
            if (parseFloat(document.getElementById("carat-min").value) > parseFloat(caratMax)) {
                document.getElementById("carat-min").value = caratMin;
            }
            if (parseFloat(document.getElementById("carat-min").value) > parseFloat(document.getElementById("carat-max").value)) {
                document.getElementById("carat-min").value = caratMin;
            }
            document.getElementById("hdnTmpCaratMin").value = parseFloat(document.getElementById("carat-min").value);
            document.getElementById("hdnTmpCaratMax").value = parseFloat(document.getElementById("carat-max").value);
            var minval = document.getElementById("hdnTmpCaratMin").value;
            var maxval = document.getElementById("hdnTmpCaratMax").value;
            $("#carat-min").val(minval);
            $("#carat-max").val(maxval);
            var $slider = $("#slider-carat");
            $slider.slider("values", 0, minval);
            $slider.slider("values", 1, maxval);
        }


